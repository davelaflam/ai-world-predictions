{
  "name": "ai-world-predictions",
  "version": "1.0.0",
  "description": "The AI World Predictions App is designed to provide AI-driven predictions for world events using a Retrieval-Augmented Generation (RAG) approach. It uses data from crawled news sources and prediction markets (Kalshi, Polymarket), stores it in Pinecone, and applies OpenAI's GPT-4o for reasoning and prediction.",
  "main": "./client/App.tsx",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ignore-pattern node_modules --ignore-pattern dist",
    "lint-fix": "eslint --ignore-pattern node_modules --ignore-pattern dist --fix .",
    "start": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/server.ts & webpack-dev-server --hot --progress --color",
    "server": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/server.ts",
    "test": "jest",
    "crawler:entertainment": "tsx ./server/webScraping/crawlers/entertainmentCrawler.ts",
    "crawler:politics": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/webScraping/crawlers/politicsCrawler.ts",
    "crawler:sports": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/webScraping/crawlers/sportsCrawler.ts",
    "crawler:world-news": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/webScraping/crawlers/worldNewsCrawler.ts",
    "ingest": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/services/pineconeIngestAll.ts",
    "ingest:entertainment": "pnpm ingest entertainment",
    "ingest:politics": "pnpm ingest politics",
    "ingest:sports": "pnpm ingest sports",
    "ingest:world-news": "pnpm ingest world-news",
    "ingest:all": "pnpm ingest all",
    "upsert-batch": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./offline/upsert-batch.ts"
  },
  "keywords": [
    "AI",
    "GPT-4o",
    "OpenAI",
    "RAG",
    "retrieval-augmented-generation",
    "Kalshi",
    "Polymarket",
    "world-predictions",
    "prediction-markets",
    "pinecone",
    "vector-search",
    "multi-agent",
    "react",
    "express",
    "nodejs",
    "typescript",
    "firecrawl",
    "web-scraping",
    "gpt",
    "ai-app",
    "real-time-predictions",
    "structured-data",
    "economic-predictions",
    "news-analysis",
    "entertainment-news",
    "council-mode",
    "deep-prediction",
    "fast-mode",
    "ai-inference",
    "finance-ai",
    "event-prediction",
    "machine-learning"
  ],
  "author": "Dave LaFlam",
  "license": "ISC",
